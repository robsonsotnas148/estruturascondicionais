nome = str(input('qual é o seu nome? '))
if nome == 'robson':
    print('que nome lindoooo')
elif nome == 'eridalberto':
    print('que nome feio kkkkk')
elif nome in 'pedro railson rivaldo edinailza mateus fernanda roni hilda':
    print('que nome legal você tem')
print('{}, tenha um bom dia!!! '.format(nome))
